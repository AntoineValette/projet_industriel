          "rawSql": "SELECT \n    DATE_TRUNC('hour', TO_TIMESTAMP(SPLIT_PART(date, ' ', 1) || ' ' || SPLIT_PART(date, ' ', 3), 'DD/MM/YYYY HH24:MI')) AS time,\n    \n    AVG(trafic_entrant_debit_raw) / (1024*1024) AS \"Average Incoming Traffic Speed (Mbps)\",\n    AVG(trafic_sortant_debit_raw) / (1024*1024) AS \"Average Outgoing Traffic Speed (Mbps)\",\n    \n    AVG(AVG(trafic_entrant_debit_raw) / (1024*1024)) \n        OVER (PARTITION BY 1 ORDER BY DATE_TRUNC('hour', TO_TIMESTAMP(SPLIT_PART(date, ' ', 1) || ' ' || SPLIT_PART(date, ' ', 3), 'DD/MM/YYYY HH24:MI'))) \n        AS \"6-Day Moving Average of Incoming Speed (Mbps)\",\n    \n    AVG(AVG(trafic_sortant_debit_raw) / (1024*1024)) \n        OVER (PARTITION BY 1 ORDER BY DATE_TRUNC('hour', TO_TIMESTAMP(SPLIT_PART(date, ' ', 1) || ' ' || SPLIT_PART(date, ' ', 3), 'DD/MM/YYYY HH24:MI'))) \n        AS \"6-Day Moving Average of Outgoing Speed (Mbps)\"\n    \nFROM myreport_reseau\nGROUP BY time\nORDER BY time;\n",
